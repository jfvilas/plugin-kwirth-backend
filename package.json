    "@backstage/integration": "^1.13.0",
    "@jfvilas/kwirth-common": "0.3.55",
    "@types/express": "^4.17.6",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "node-fetch": "^2.6.5",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.26.11",
    "@types/node-fetch": "^2.5.12",
    "@types/supertest": "^2.0.8",
    "supertest": "^6.1.3"
